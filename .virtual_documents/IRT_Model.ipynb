def sample_w(P, I, alpha, beta, theta):
    w = np.zeros((P, I))
    for p in range(len(w)):
        for i in range(len(w[p])):
            w[p][i] = random_polyagamma(z = alpha[i] * theta[p] + beta[i])

    return w


